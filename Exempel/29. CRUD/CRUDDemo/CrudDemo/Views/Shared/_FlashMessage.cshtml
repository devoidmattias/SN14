@section Scripts{
    <script src="~/Scripts/toastr.min.js"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            if ('@TempData["success"]' != null) {
                toastr[success]("@TempData["success"]")
            }
            if ('@TempData["error"]' != null) {
                toastr[error]("@TempData["error"]", "Fel")
            }

        });
    </script>
}


